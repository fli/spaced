{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "target": "es6",
    "lib": [
      "dom", "es2017", "scripthost"
    ]
  },
  "include": [
    "@types"
  ]
}